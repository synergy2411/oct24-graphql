generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model User {
  id       String @id @default(auto()) @map("_id") @db.ObjectId
  name     String
  age      Int
  email    String @unique
  password String
  role     Role

  @@index([name, age])
}

enum Role {
  ADMIN
  MANAGER
  DEVELOPER
}
